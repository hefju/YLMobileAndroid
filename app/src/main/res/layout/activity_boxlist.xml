<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="ylescort.ylmobileandroid.boxlist">

    <TextView
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        android:id="@+id/boxlv_tv_order"
        android:text="序号"/>
    <TextView
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:id="@+id/boxlv_tv_Number"
        android:text="箱编号"
        android:layout_toRightOf="@+id/boxlv_tv_order"/>
    <TextView
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:id="@+id/boxlv_tv_getorgive"
        android:text="收/送"
        android:layout_toRightOf="@+id/boxlv_tv_Number"/>
    <TextView
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:id="@+id/boxlv_tv_emporfull"
        android:text="空/实"
        android:layout_toRightOf="@+id/boxlv_tv_getorgive"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/boxlv_tv_staut"
        android:text="箱类型"
        android:layout_toRightOf="@+id/boxlv_tv_emporfull"/>
</RelativeLayout>
