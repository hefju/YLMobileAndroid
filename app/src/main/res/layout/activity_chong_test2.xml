<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="ylescort.ylmobileandroid.chongTest2">


    <TextView android:text="@string/hello_world" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="获取全部员工"
        android:id="@+id/button"
        android:onClick="chongtest_emp"
        android:layout_below="@+id/textView"
        android:layout_alignParentStart="true"
        android:layout_x="11dp"
        android:layout_y="11dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="获取全部箱"
        android:onClick="chongtest_box"
        android:id="@+id/button5"
        android:layout_alignTop="@+id/button"
        android:layout_centerHorizontal="true"
        android:layout_x="150dp"
        android:layout_y="77dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="获取全部网点"
        android:id="@+id/button6"
        android:onClick="chongtest_site"
        android:layout_below="@+id/button"
        android:layout_alignParentStart="true"
        android:layout_x="11dp"
        android:layout_y="77dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="获取全部客户"
        android:id="@+id/button7"
        android:onClick="chongtest_client"
        android:layout_alignTop="@+id/button6"
        android:layout_alignStart="@+id/button5"
        android:layout_x="150dp"
        android:layout_y="11dp" />

    <ListView
        android:layout_width="wrap_content"
        android:layout_height="325dp"
        android:id="@+id/listView"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="159dp"
        android:layout_x="-3dp"
        android:layout_y="220dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="上传任务"
        android:id="@+id/button8"
        android:layout_x="13dp"
        android:layout_y="145dp"
        android:onClick="upload_Task"
        android:nestedScrollingEnabled="false" />

</AbsoluteLayout>
