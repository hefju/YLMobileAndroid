<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".Login">


    <TextView
        android:text="@string/app_name"
        android:id="@+id/Log_TV_Title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="@string/UserName"
        android:id="@+id/Log_TV_Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:layout_alignBaseline="@+id/Log_ET_Name"
        android:layout_alignBottom="@+id/Log_ET_Name"
        android:layout_alignParentStart="true" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Log_ET_Name"
        android:text="200097"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="100dp"
        android:textSize="30dp"
        android:layout_toEndOf="@+id/Log_TV_Name" />
    <TextView
        android:text="@string/UserPassword"
        android:id="@+id/Log_TV_PassWord"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="30dp"
        android:layout_below="@+id/Log_TV_Name"
        android:layout_alignParentStart="true" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Log_ET_PassWord"
        android:text="200097"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="150dp"
        android:textSize="30dp"
        android:layout_toEndOf="@+id/Log_TV_Name" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Log_BN_Ent"
        android:text="@string/Log_Ent"
        android:onClick="LoginEnter"
        android:layout_below="@+id/Log_ET_PassWord"
        android:layout_alignEnd="@+id/Log_TV_PassWord" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Log_BN_Cal"
        android:text="@string/Log_Can"
        android:layout_below="@+id/Log_ET_PassWord"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Log_BN_Update"
        android:text="@string/Log_Update"
        android:layout_below="@+id/Log_ET_PassWord"
        android:layout_alignEnd="@+id/Log_TV_Title" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btnTest1"
        android:text="巨测试1"
        android:layout_below="@+id/Log_BN_Ent"
        android:layout_alignEnd="@+id/Log_TV_PassWord" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btnTest2"
        android:text="巨测试2"
        android:layout_below="@+id/Log_BN_Cal"
        android:layout_centerHorizontal="true" />


</RelativeLayout>
