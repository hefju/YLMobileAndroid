<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
<LinearLayout
    android:layout_width="fill_parent"
    android:layout_height="wrap_content">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/vault_in_detail_boxitme_boxname"
        android:text="@string/boxitem_Boxid"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20sp"
        android:id="@+id/vault_in_detail_boxitme_boxstaut"
        android:text="@string/boxitem_Boxid"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20sp"
        android:id="@+id/vault_in_detail_boxitme_boxtype"
        android:text="@string/boxitem_Boxid"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20sp"
        android:id="@+id/vault_in_detail_boxitme_boxtask"
        android:text="@string/boxitem_Boxid"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20sp"
        android:id="@+id/vault_in_detail_boxitme_boxcheck"
        android:text="@string/boxitem_Boxid"/>
    </LinearLayout>
</LinearLayout>